Subject: Object
{
    "id": String,
    "name": String,
    "icon": String,
    "color": String
}

Question: Object
{
    "id": String,
    "subjectId": String,
    "difficulty": Number,
    "text": String,
    "isAnswered": Boolean
}

Answer: Object
{
    "id": String,
    "questionId": String,
    "isCorrect": Boolean,
    "text": String
}

Player: Object
{
    "id": String
    "name": String,
    "avatar": String,
    "score": Score[]
}

Score: Object
{
    subjectId: String,
    points: Number
}

Turn: Object
{
    turn: Number,
    playerId: String,
    isPlayed: Boolean
}